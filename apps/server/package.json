{
  "name": "@ikki/server",
  "version": "1.0.50",
  "scripts": {
    "dev": "bun run --watch src/server.ts",
    "dev:debug": "bun run --watch --inspect src/server.ts",
    "test": "bun run test:unit && bun run coverage",
    "test:unit": "bun test",
    "build": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile ./dist/server ./src/server.ts",
    "db:generate": "bunx --bun drizzle-kit generate",
    "db:migrate": "bunx --bun drizzle-kit migrate",
    "db:studio": "bunx --bun drizzle-kit studio",
    "seed": "bun run scripts/seed.ts",
    "lint": "eslint --report-unused-disable-directives --max-warnings 0 .",
    "lint:fix": "eslint --fix --max-warnings 0 .",
    "format": "prettier --write .",
    "knip": "knip --config knip.jsonc",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@axiomhq/js": "^1.4.0",
    "@axiomhq/pino": "^1.4.0",
    "@elysiajs/cors": "^1.4.1",
    "@elysiajs/openapi": "^1.4.14",
    "@elysiajs/opentelemetry": "^1.4.10",
    "@kubiks/otel-drizzle": "^2.1.0",
    "@loglayer/plugin-opentelemetry": "^2.1.15",
    "@loglayer/transport-axiom": "^2.1.16",
    "@loglayer/transport-pretty-terminal": "^4.1.9",
    "@loglayer/transport-simple-pretty-terminal": "^2.3.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.211.0",
    "@opentelemetry/instrumentation-pino": "^0.57.0",
    "@opentelemetry/sdk-trace-base": "^2.5.1",
    "@opentelemetry/sdk-trace-node": "^2.5.1",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "bcrypt": "^6.0.0",
    "cache-manager": "^7.2.8",
    "drizzle-orm": "^0.44.7",
    "drizzle-zod": "^0.8.3",
    "elysia": "^1.4.25",
    "es-toolkit": "^1.44.0",
    "jsonwebtoken": "^9.0.3",
    "loglayer": "^8.5.0",
    "ms": "^2.1.3",
    "pino": "^10.3.1",
    "pino-pretty": "^13.1.3",
    "postgres": "^3.4.8",
    "serialize-error": "^13.0.1",
    "zod": "^4.3.6",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.1",
    "@scalar/types": "^0.6.3",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/ms": "^2.1.0",
    "bun-types": "latest",
    "drizzle-kit": "^0.31.9",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.24.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-sonarjs": "^3.0.7",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-unused-imports": "^4.4.1",
    "globals": "^17.3.0",
    "knip": "^5.83.1",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0"
  }
}
